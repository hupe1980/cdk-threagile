  },
  "peerDependencies": {
    "constructs": "^10.1.42"
  },
  "dependencies": {
    "adm-zip": "0.5.6",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "execa": "^5",
    "form-data": "^4.0.0",
    "fs-extra": "^10.1.0",
    "ts-node": "^10.8.1",
    "uuid": "^8.3.2",
    "yaml": "^2.1.1",
    "yargs": "^17.5.1"
  },
  "bundledDependencies": [
    "adm-zip",
    "axios",
    "dotenv",
    "execa",
    "form-data",
    "fs-extra",
    "ts-node",
    "uuid",
